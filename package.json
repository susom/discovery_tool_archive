{
  "name": "edu.stanford.discoverytool.ourvoice",
  "version": "1.0.55",
  "displayName": "The Discovery Tool",
  "cordova": {
    "platforms": [
      "ios"
    ],
    "plugins": {
      "cordova-plugin-ios-location-permissions": {
        "LOCATION_WHEN_IN_USAGE_DESCRIPTION": "This app needs contact access",
        "LOCATION_ALWAYS_AND_WHEN_IN_USAGE_DESCRIPTION": "This app needs contact access"
      },
      "cordova-plugin-x-toast": {},
      "cordova-plugin-insomnia": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-compat": {},
      "cordova-plugin-dialogs": {},
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      },
      "cordova-plugin-ios-disableshaketoedit": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-camera": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-file": {},
      "cordova-plugin-media": {
        "KEEP_AVAUDIOSESSION_ALWAYS_ACTIVE": "NO"
      },
      "cordova-plugin-media-capture": {},
      "cordova-plugin-screen-orientation": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-sqlite-2": {},
      "cordova-plugin-wkwebview-file-xhr": {}
    }
  },
  "dependencies": {
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-dialogs": "^2.0.2",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-insomnia": "^4.3.0",
    "cordova-plugin-ios-disableshaketoedit": "^1.0.0",
    "cordova-plugin-ios-location-permissions": "1.0.0",
    "cordova-plugin-network-information": "^3.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-x-toast": "^2.7.2"
  },
  "devDependencies": {
    "cordova-ios": "^7.0.0",
    "cordova-plugin-camera": "^6.0.0",
    "cordova-plugin-device": "^2.1.0",
    "cordova-plugin-file": "^8.0.0",
    "cordova-plugin-media": "^6.1.0",
    "cordova-plugin-media-capture": "^5.0.0",
    "cordova-plugin-screen-orientation": "^3.0.3",
    "cordova-plugin-splashscreen": "^6.0.2",
    "cordova-plugin-sqlite-2": "^1.0.7",
    "cordova-plugin-whitelist": "^1.3.5",
    "cordova-plugin-wkwebview-file-xhr": "^3.1.0"
  }
}